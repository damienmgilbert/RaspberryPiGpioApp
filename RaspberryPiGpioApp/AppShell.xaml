<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RaspberryPiGpioApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:RaspberryPiGpioApp"
    xmlns:views="clr-namespace:RaspberryPiGpioApp.Views"
    Title="Raspberry Pi 4 Gpio App"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:HomePage}"
        Route="Home" />

    <ShellContent
        Title="Board"
        ContentTemplate="{DataTemplate views:BoardPage}"
        Route="Board" />

</Shell>
